{
  "hosting": {
	 "target": "outline-website",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
		{
  		 "source": "/app",
  		 "destination": "/app.html"
	   },
      {
        "source": "**",
        "destination": "/index.html"
	  }
    ]
  }
}
